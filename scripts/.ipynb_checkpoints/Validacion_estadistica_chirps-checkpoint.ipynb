    "pinedo_actual_season = pinedo_actual.groupby('time.season')\n",
    "pinedo_predicted_season = pinedo_predicted.groupby('time.season')\n",
    "\n",
    "hondo_actual_season = hondo_actual.groupby('time.season')\n",
    "hondo_predicted_season = hondo_predicted.groupby('time.season')\n",
    "\n",
    "angela_actual_season = angela_actual.groupby('time.season')\n",
    "angela_predicted_season = angela_predicted.groupby('time.season')\n",
    "\n",
    "archivos_actual = [pinedo_actual_season,hondo_actual_season,angela_actual_season]\n",
    "archivos_predicted = [pinedo_predicted_season,hondo_predicted_season,angela_predicted_season]\n",
    "\n",
    "stations = [0,1,2]\n",
    "seasons = ['DJF', 'MAM', 'JJA', 'SON']\n",
    "\n",
    "rms_pinedo=[0,0,0,0]\n",
    "rms_hondo=[0,0,0,0]\n",
    "rms_angela=[0,0,0,0]\n",
    "stations_names=['pinedo','hondo','angela']\n",
    "\n",
    "rms_seasons=[rms_pinedo,rms_hondo,rms_angela]\n",
    "\n",
    "from sklearn.metrics import mean_squared_error\n",
    "for station in stations:\n",
    "    for index, season in enumerate(seasons):\n",
    "        rms_seasons[station][index] = mean_squared_error(archivos_actual[station][season].values, archivos_predicted[station][season].values, squared=False)/(archivos_actual[station][season].mean()).values\n",
    "\n",
    "print('*'*100)\n",
    "print(' '*50,'NRMSE')\n",
    "print('*'*20,'DJF','*'*15,'MAM','*'*15,'JJA','*'*15,'SON','*'*20)\n",
    "for station in np.arange(len(rms_seasons)):\n",
    "    print(f'valores para {stations_names[station]}',rms_seasons[station])     \n",
    "\n",
    "bias_pinedo = [0,0,0,0]\n",
    "bias_hondo = [0,0,0,0]\n",
    "bias_angela = [0,0,0,0]\n",
    "bias_seasons = [bias_pinedo,bias_hondo,bias_angela]\n",
    "\n",
    "\n",
    "for station in stations:\n",
    "    for index, season in enumerate(seasons):\n",
    "        bias_seasons[station][index] = ((archivos_actual[station][season].values - archivos_predicted[station][season]).mean()).values/(archivos_actual[station][season].mean()).values\n",
    "print('*'*100)\n",
    "print(' '*50,'BIAS')\n",
    "print('*'*20,'DJF','*'*15,'MAM','*'*15,'JJA','*'*15,'SON','*'*20)\n",
    "for station in np.arange(len(bias_seasons)):\n",
    "    print(f'valores para {stations_names[station]}',bias_seasons[station])\n",
    "\n",
    "print('*'*100)\n",
    "print(' '*50,'CORR')\n",
    "print('*'*20,'DJF','*'*15,'MAM','*'*15,'JJA','*'*15,'SON','*'*20)\n",
    "from scipy.stats import pearsonr\n",
    "\n",
    "corr_pinedo = [0,0,0,0]\n",
    "corr_hondo = [0,0,0,0]\n",
    "corr_angela = [0,0,0,0]\n",
    "corr_seasons = [corr_pinedo,corr_hondo,corr_angela]\n",
    "\n",
    "\n",
    "for station in stations:\n",
    "    for index, season in enumerate(seasons):\n",
    "        corr_seasons[station][index],_ = pearsonr(archivos_actual[station][season].values, archivos_predicted[station][season].values)\n",
    "\n",
    "for station in np.arange(len(corr_seasons)):\n",
