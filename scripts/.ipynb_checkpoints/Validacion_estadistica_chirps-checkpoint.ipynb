    "# Me importo los valores de chirps para la zona a utilizar:\n",
    "\n",
    "pathDatosChirps = \f"{workspace}tesis/datos/crudos/netcdf/\"\n",
    "pathDatosChirps2019 = pathDatosChirps + \"chirps-v2.0.2019.days_p05.nc\"\n",
    "pathDatosChirps2020 = pathDatosChirps + \"chirps-v2.0.2020.days_p05.nc\"\n",
    "pathDatosChirps2021 = pathDatosChirps + \"chirps-v2.0.2021.days_p05.nc\"\n",
    "pathDatosChirps2022 = pathDatosChirps + \"chirps-v2.0.2022.days_p05.nc\"\n",
    "\n",
    "chirps2019 = xr.open_dataset(pathDatosChirps2019, engine=\"netcdf4\")\n",
    "chirps2020 = xr.open_dataset(pathDatosChirps2020, engine=\"netcdf4\")\n",
    "chirps2021 = xr.open_dataset(pathDatosChirps2021, engine=\"netcdf4\")\n",
    "chirps2022 = xr.open_dataset(pathDatosChirps2022, engine=\"netcdf4\")\n",
    "\n",
    "# Recorto a la zona de interes y a los días que me interesan\n",
    "\n",
    "chirps2019_Argentina = chirps2019.sel(latitude=slice(-32,-23))\n",
    "chirps2019_Argentina = chirps2019_Argentina.sel(longitude=slice(-72,-56))\n",
    "chirps2019_Argentina = chirps2019_Argentina.sel(time=slice('2019-02-05T00:00:00.000000000','2022-01-19T00:00:00.000000000'))\n",
    "\n",
    "chirps2020_Argentina = chirps2020.sel(latitude=slice(-32,-23))\n",
    "chirps2020_Argentina = chirps2020_Argentina.sel(longitude=slice(-72,-56))\n",
    "chirps2020_Argentina = chirps2020_Argentina.sel(time=slice('2019-02-05T00:00:00.000000000','2022-01-19T00:00:00.000000000'))\n",
    "\n",
    "chirps2021_Argentina = chirps2021.sel(latitude=slice(-32,-23))\n",
    "chirps2021_Argentina = chirps2021_Argentina.sel(longitude=slice(-72,-56))\n",
    "chirps2021_Argentina = chirps2021_Argentina.sel(time=slice('2019-02-05T00:00:00.000000000','2022-01-19T00:00:00.000000000'))\n",
    "\n",
    "chirps2022_Argentina = chirps2022.sel(latitude=slice(-32,-23))\n",
    "chirps2022_Argentina = chirps2022_Argentina.sel(longitude=slice(-72,-56))\n",
    "chirps2022_Argentina = chirps2022_Argentina.sel(time=slice('2019-02-05T00:00:00.000000000','2022-01-19T00:00:00.000000000'))\n",
    "\n",
    "# Concateno los archivos a través de la dimensión tiempo para que me quede toda la info de 2019 a 2022.\n",
    "chirps20192022_Argentina = xr.combine_nested(datasets=[chirps2019_Argentina,chirps2020_Argentina,chirps2021_Argentina,chirps2022_Argentina],concat_dim='time',combine_attrs=\"drop_conflicts\")\n",
    "chirps20192022_Argentina.to_netcdf(path=\f"{workspace}tesis/datos/procesados/chirps20192022_validacion.nc\", engine='netcdf4')\n",
    "chirps20192022_Argentina"
   ]
