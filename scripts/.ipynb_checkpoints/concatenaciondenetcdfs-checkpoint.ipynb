   "source": [
    "#Abro y concateno los archivos de MSWEP mensuales (uno por mes), previamente son recortados al área de interés.\n",
    "#debería guardar el archivo procesado en un archivo propio.\n",
    "\n",
    "df_archivoCompleto = xr.open_dataset(path+archivos[1])\n",
    "df_archivoCompleto = df_archivoCompleto.sel(lat=slice(-32,-23))\n",
    "df_archivoCompleto = df_archivoCompleto.sel(lon=slice(-72,-56))\n",
    "\n",
    "for index, archivo in enumerate([x for x in archivos if x is not archivos[0] and x is not archivos[1]]):\n",
    "    df_archivo = xr.open_dataset(path+archivo, engine=\"netcdf4\")\n",
    "    df_archivo = df_archivo.sel(lat=slice(-32,-23))\n",
    "    df_archivo = df_archivo.sel(lon=slice(-72,-56))\n",
    "    df_archivoCompleto = xr.combine_nested(datasets=[df_archivoCompleto, df_archivo],concat_dim='time',combine_attrs=\"drop_conflicts\")\n",
    "\n",
    "#le agrego la meta data\n",
    "#df_archivoCompleto.attrs['units'] = 'mm/day'\n",
    "#df_archivoCompleto.attrs['standard_name'] = 'convective precipitation rate'\n",
    "#df_archivoCompleto.attrs['long_name'] = 'Multi-Source Weighted-Ensemble Precipitation'\n",
    "#df_archivoCompleto.attrs['time_step'] = 'day'\n",
    "\n",
    "#lo salvo en un archivo en la carpeta de procesados"
   ]
